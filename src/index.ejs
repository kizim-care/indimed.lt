    let textIndex = 0;
    let charIndex = 0;
  
    function typeText() {
      if (charIndex < texts[textIndex].textContent.length) {
        header.textContent += texts[textIndex].textContent.charAt(charIndex);
        charIndex++;
        setTimeout(typeText, 40);
      } else {
